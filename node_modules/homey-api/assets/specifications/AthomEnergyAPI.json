{
  "host": "energy-api.athom.com",
  "basePath": "/api/v1/",
  "operations": {
    "getZone": {
      "path": "/zone",
      "method": "get",
      "parameters": {
        "latitude": {
          "in": "query",
          "type": "string",
          "required": true
        },
        "longitude": {
          "in": "query",
          "type": "string",
          "required": true
        },
        "secret": {
          "in": "query",
          "type": "string"
        }
      }
    },
    "getZones": {
      "path": "/zones",
      "method": "get",
      "parameters": {
        "secret": {
          "in": "query",
          "type": "string"
        }
      }
    },
    "getDayAheadPricesElectricity": {
      "path": "/day-ahead-prices/electricity",
      "method": "get",
      "parameters": {
        "zoneId": {
          "in": "query",
          "type": "string",
          "required": true
        },
        "zoneVersion": {
          "in": "query",
          "type": "string",
          "required": true
        },
        "date": {
          "in": "query",
          "type": "string",
          "required": true
        },
        "timezone": {
          "in": "query",
          "type": "string",
          "required": true
        },
        "currency": {
          "in": "query",
          "type": "string"
        },
        "interval": {
          "in": "query",
          "type": "string"
        },
        "secret": {
          "in": "query",
          "type": "string"
        }
      }
    }
  }
}